{
    "name": "glide_final",
    "version": "1.0.0",
    "description": "Arduino control application",
    "scripts": {
        "start:backend": "cd backend && ./start_backend.sh",
        "start:frontend": "cd frontend && npm run dev",
        "start:electron": "cd electron && npm start",
        "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\" \"npm run start:electron\""
    },
    "devDependencies": {
        "concurrently": "^7.0.0"
    }
}
